<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <StartupObject>MikuSharp.Program</StartupObject>
    <ApplicationIcon />
    <Version>2.0.0</Version>
    <Authors>MikuSharp Team</Authors>
    <RepositoryUrl>https://github.com/Speyd3r/MikuSharp/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/Speyd3r/MikuSharp/</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Full Hatsune Miku Discord bot C# Rewrite!</Description>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="MusicCommands\**" />
    <EmbeddedResource Remove="MusicCommands\**" />
    <None Remove="MusicCommands\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AlbumArtExtraction" Version="1.0.2" />
    <PackageReference Include="AngleSharp" Version="1.0.0-alpha-844" />
    <PackageReference Include="DisCatSharp" Version="9.8.3-nightly-00121" />
    <PackageReference Include="DisCatSharp.ApplicationCommands" Version="9.8.3-nightly-00121" />
    <PackageReference Include="DisCatSharp.CommandsNext" Version="9.8.3-nightly-00121" />
    <PackageReference Include="DisCatSharp.Interactivity" Version="9.8.3-nightly-00121" />
    <PackageReference Include="DisCatSharp.Lavalink" Version="9.8.3-nightly-00121" />
    <PackageReference Include="DiscordBotsList.Api" Version="1.4.0" />
    <PackageReference Include="FluentFTP" Version="35.0.4" />
    <PackageReference Include="Google.Apis" Version="1.55.0" />
    <PackageReference Include="Google.Apis.YouTube.v3" Version="1.55.0.2449" />
    <PackageReference Include="Kitsu" Version="1.4.2" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0-rc.1.21451.13" />
    <PackageReference Include="Mime" Version="3.2.3" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Npgsql" Version="6.0.0-preview7" />
    <PackageReference Include="NYoutubeDL" Version="0.11.2" />
    <PackageReference Include="Weeb.net" Version="1.0.7" />
    <PackageReference Include="YoutubeExplode" Version="6.0.5" />
  </ItemGroup>

  <ItemGroup>
    <None Update="config.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
